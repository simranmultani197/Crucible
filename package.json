{
  "name": "termless",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true})\"",
    "dev": "next dev",
    "dev:clean": "npm run clean && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "microvm:probe": "node scripts/microvmctl.js probe",
    "microvm:setup:linux": "bash scripts/linux/install-worker.sh",
    "microvm:setup:macos": "bash scripts/macos/install-krunvm-worker.sh",
    "microvm:setup:macos:localvm": "bash scripts/macos/install-krunvm-worker.sh",
    "microvm:setup:windows": "powershell -ExecutionPolicy Bypass -File scripts/windows/install-hyperv-worker.ps1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@e2b/code-interpreter": "^2.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.95.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "next": "14.2.35",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "@tailwindcss/typography": "^0.5.9",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20 <25"
  }
}
